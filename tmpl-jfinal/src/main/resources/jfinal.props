
modules+= maven

inits+=


depends+='''
    com.github.zqq90.webit-script:webit-script:1.4.0
    mysql:mysql-connector-java:5.1.29
    c3p0:c3p0:0.9.1.2
    com.jfinal:jfinal:1.5
'''


testDepends+='''
  junit:junit:4.8.2
'''


